{
	"name": "<%= kebabTitle %>",
	"version": "0.0.0",
	"description": "<%= description %>",<% if (isOpenSource) { %>
	"license": "MIT",
	"repository": "<%= githubUsername %>/<%= kebabTitle %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },<% } %>
	"scripts": {
    "postinstall": "check-node-version --package",
		"start": "gulp dev --require @std/esm",
		"build": "gulp build --require @std/esm",
		"bundle": "gulp bundle --require @std/esm",
    "pretest": "npm run build",
    "test": "ava"
	},
	"@std/esm": "all",
	"engines": {
		"node": ">=8.6"
	},
	"dependencies": {
		"chrome-promise": "^2.1.1"
	},
	"devDependencies": {
		"@std/esm": "^0.15.0",
		"ava": "^0.24.0",
		"babel-eslint": "^8.0.3",
		"babel-loader": "^7.1.2",
		"babel-plugin-lodash": "^3.3.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-export-extensions": "^6.22.0",
		"babel-plugin-transform-function-bind": "^6.22.0",
		"babel-preset-env": "^1.6.1",
    "check-node-version": "^3.1.1",
		"del": "^3.0.0",
		"dotenv": "^4.0.0",
		"dotenv-webpack": "^1.5.4",
		"eslint-formatter-pretty": "^1.3.0",
		"eslint-plugin-babel": "^4.1.2",
		"gulp": "gulpjs/gulp.git#4.0",
		"gulp-add-src": "^0.2.0",
		"gulp-changed": "^3.1.1",
		"gulp-eslint": "^4.0.0",
		"gulp-if": "^2.0.2",
		"gulp-json-editor": "^2.2.1",
		"gulp-notify": "^3.0.0",
		"gulp-sass": "^3.1.0",
		"gulp-zip": "^4.0.0",
    "puppeteer": "^0.13.0",
		"sass-module-importer": "^1.4.0",
		"socket.io": "^2.0.4",
		"socket.io-client": "^2.0.4",
		"vinyl-named": "^1.1.0",
		"webpack-stream": "^4.0.0"
	}
}
